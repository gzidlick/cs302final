{
	"name": "Password Manager",
	"version": "0.1",
	"description": "Welp. It's a password manager.",
	"permissions": ["storage", "declarativeContent"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action":{
		"default_title": "Password Manager",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"manifest_version": 2
}
